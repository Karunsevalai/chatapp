{
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "headers": [ { "source": "/api/(.*)", "headers": [ { "key": "Access-Control-Allow-Origin", "value": "https://chatapp.mysqft.in" }, { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,OPTIONS" }, { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }, { "key": "Access-Control-Allow-Credentials", "value": "true" } ] } ],
  "routes": [
    { "src": "/(.*)", "dest": "server.js" }
  ]

}
